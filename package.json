{
  "name": "ymlgen",
  "displayName": "ymlgen",
  "publisher": "linq2js",
  "description": "A generator for YML/YAML files",
  "repository": {
    "url": "https://github.com/linq2js/ymlgen.git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "icon": "images/yml.png",
  "categories": [
    "Other"
  ],
  "keywords": [
    "Template",
    "Generator",
    "YAML",
    "YML",
    "Javascript"
  ],
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ymlgen.enable",
        "title": "ymlgen: enable"
      },
      {
        "command": "ymlgen.disable",
        "title": "ymlgen: disable"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build": "yarn vscode:prepublish",
    "package": "mkdir dist || yarn build && vsce package -o dist/ymlgen.vsix --yarn",
    "deploy:local": "yarn package && code --install-extension dist/ymlgen.vsix --force"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "yamljs": "^0.3.0",
    "zod": "^3.18.0"
  }
}
